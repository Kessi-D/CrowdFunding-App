	<main>

		<header>
			<div class="px-3 py-2 bg-dark text-white">
			  <div class="container">
				<div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
					<a class="navbar-brand" href="/"><img src="https://library.maximnyansa.com/static/media/maxim_logo.f94113f2.png" alt=""></a>
				  <a href="/" class="d-flex align-items-center my-2 my-lg-0 me-lg-auto text-white text-decoration-none">
				  </a>
		
				  <div class="text-end">
					  <small>Hello, <%= userEmail %></small>
					<a href="/financial/logout" type="button" class="btn btn-warning">Logout</a>
				  </div>
				</div>
			  </div>
			</div>
		</header>
	
		<div class="album py-5 bg-light">
			<div class="container">
		
		
			  <h2 class="pb-2 border-bottom">Update financial details of Projects</h2><br>
			  <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">

				<% for(var i=0; i< projects.length; i++) { %>
					<div class="col">
						<div class="card shadow-sm">
						  <img src="/images/final_stage_pics/<%= projects[i].finalImages[0] %>" alt="" style="height: 250px;">
			  
						  <div class="card-body">
							<p class="card-text text-truncate"><%= projects[i].description %></p>
							<div class="d-flex justify-content-between align-items-center">
							  <div class="btn-group">
								<a type="button" href="/financial/view-project/<%= projects[i]._id %>" class="btn  btn-outline-secondary">View</a>
							  </div>
							  <small class="text-muted">9 mins</small>
							</div>
						  </div>
						</div>
					</div>

				<% } %>
		
				
		   
		
			  </div>
			</div>
		  </div>
	
	
	</main>
	
	<div class="container">
		<footer class="row row-cols-5 py-5 my-5 border-top">
		  <div class="col">
			<a href="/" class="d-flex align-items-center mb-3 link-dark text-decoration-none">
				<a class="navbar-brand" href="/"><img src="https://library.maximnyansa.com/static/media/maxim_logo.f94113f2.png" alt=""></a>
			</a>
			<p class="text-muted">&copy; 2021</p>
		  </div>
	  
